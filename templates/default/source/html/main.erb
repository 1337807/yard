<div class="section <%= generator_name %>">
  <span>[<a class="source_link" href="#">View source</a>]</span>
  <div class="source_code">
    <pre class="lines"><%= "\n\n\n" %><%= h format_lines(object) %></pre>
    <pre class="code"><span class="file"># File '<%= h object.file %>', line <%= object.line %></span><%= "\n\n" %><%= html_syntax_highlight object.source %></pre>
    <div class="clear"></div>
  </div>
</div>