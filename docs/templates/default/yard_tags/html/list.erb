<% [["Tag", all_tags], ["Directive", all_directives]].each do |(name, list)| %>
<h2><%= name %> List</h2>

<% list.each do |tag| %>
<div class="method_details">
  <h3 class="signature" id="<%= tag.name %>" toc-title="<%= tag_link_name(tag) %>">
    <%= tag_signature tag %>
  </h3>
  <%= yieldall :object => tag.object %>
</div>
<% end %>

<% end%>
