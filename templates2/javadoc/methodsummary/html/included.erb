<p class="included">
  <table border="1" width="100%">
  <% included_meths_by_module do |klass, meths| %>
    <tr background="#ddddff">
      <th><%= visibility.to_s.capitalize %> <%= scope.to_s.capitalize %> Methods included from <%= linkify klass %></th>
    </tr>
    <tr>
      <td><%= format_object_name_list meths %></td>
    </tr>
  <% end %>
  </table>
</p>
