<% if attr_listing.size > 0 %>
  <h2>Attribute Summary</h2>
  <ul class="summary">
    <% attr_listing.each do |meth| %>
      <li class="<%= meth.visibility %> <%= meth.has_tag?(:deprecated) ? 'deprecated' : '' %>"><%= signature(meth) %>
        <% if meth.has_tag?(:deprecated) %>
          <span class="summary_desc"><strong>Deprecated.</strong> <%= meth.tag(:deprecated).text %></span>
        <% elsif !meth.docstring.empty? %>
          <span class="summary_desc"><%= htmlify_line meth.docstring.summary %></span>
        <% end %>
      </li>
    <% end %>
  </ul>
<% end %>
